  },
  "peerDependencies": {
    "postcss-html": "^1.0.0",
    "stylelint": "^16.0.0"
  },
  "dependencies": {
    "@double-great/stylelint-a11y": "3.0.4",
    "defu": "6.1.4",
    "es-toolkit": "1.34.1",
