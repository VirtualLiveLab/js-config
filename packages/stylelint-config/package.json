{
  "name": "@virtual-live-lab/stylelint-config",
  "version": "3.0.7",
  "description": "Stylelint configuration for Virtual Live Lab",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./scss": {
      "types": "./dist/scss.d.ts",
      "import": "./dist/scss.js"
    },
    "./astro": {
      "types": "./dist/astro.d.ts",
      "import": "./dist/astro.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown"
  },
  "peerDependencies": {
    "postcss-html": "^1.0.0",
    "stylelint": "^16.0.0"
  },
  "peerDependenciesMeta": {
    "postcss-html": {
      "optional": true
    }
  },
  "dependencies": {
    "@double-great/stylelint-a11y": "3.4.0",
    "defu": "6.1.4",
    "stylelint-config-html": "1.1.0",
    "stylelint-config-recess-order": "7.2.0",
    "stylelint-config-sass-guidelines": "12.1.0",
    "stylelint-config-standard": "39.0.0",
    "stylelint-config-standard-scss": "15.0.1",
    "stylelint-declaration-block-no-ignored-properties": "2.8.0",
    "stylelint-value-no-unknown-custom-properties": "6.0.1"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@virtual-live-lab/eslint-config": "workspace:*",
    "@virtual-live-lab/tsconfig": "workspace:*",
    "publint": "catalog:",
    "stylelint": "16.23.1",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "unplugin-unused": "catalog:",
    "vitest": "catalog:"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "provenance": true
  },
  "keywords": [],
  "author": "VLL39 <vll39.tech@gmail.com> (https://github.com/vll39)",
  "homepage": "https://github.com/VirtualLiveLab/js-config/tree/main/#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VirtualLiveLab/js-config.git",
    "directory": "packages/stylelint-config"
  },
  "bugs": {
    "url": "https://github.com/VirtualLiveLab/js-config/issues"
  }
}
