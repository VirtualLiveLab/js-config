{
  "name": "@virtual-live-lab/eslint-config",
  "version": "2.2.18",
  "description": "ESLint configuration for Virtual Live Lab",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    },
    "./addons/*": {
      "import": "./dist/addons/*.mjs",
      "types": "./dist/addons/*.d.mts"
    },
    "./presets/*": {
      "import": "./dist/presets/*.mjs",
      "types": "./dist/presets/*.d.mts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup-node",
    "inspect": "eslint --inspect-config"
  },
  "engines": {
    "node": ">= 20.11.0 || >= 21.2.0 || >= 22.0.0"
  },
  "peerDependencies": {
    "eslint": "^8.57.0 || ^9.0.0"
  },
  "dependencies": {
    "@astrojs/compiler": "2.11.0",
    "@eslint/compat": "1.2.7",
    "@eslint/eslintrc": "3.3.0",
    "@eslint/js": "9.22.0",
    "@stylistic/eslint-plugin-ts": "4.2.0",
    "@typescript-eslint/utils": "8.27.0",
    "astro-eslint-parser": "1.2.1",
    "astrojs-compiler-sync": "1.0.1",
    "eslint": "9.22.0",
    "eslint-config-flat-gitignore": "2.1.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-perfectionist": "4.10.1",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.19",
    "eslint-plugin-tailwindcss": "3.18.0",
    "globals": "16.0.0",
    "pkg-dir": "8.0.0",
    "typescript-eslint": "8.27.0",
    "typescript-eslint-parser-for-extra-files": "0.7.0"
  },
  "devDependencies": {
    "@types/eslint": "9.6.1",
    "@types/eslint-config-prettier": "6.11.3",
    "@types/eslint__eslintrc": "2.1.2",
    "@types/eslint__js": "8.42.3",
    "@types/node": "22.13.10",
    "@virtual-live-lab/tsconfig": "workspace:*",
    "@vitest/coverage-istanbul": "3.0.8",
    "tsup": "8.4.0",
    "vitest": "3.0.8"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "provenance": true
  },
  "keywords": [],
  "author": "sushi-chaaaan <mail@sushichan.live> (https://github.com/sushi-chaaaan)",
  "homepage": "https://github.com/VirtualLiveLab/js-config/tree/main/#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/VirtualLiveLab/js-config.git",
    "directory": "packages/eslint-config"
  },
  "bugs": {
    "url": "https://github.com/VirtualLiveLab/js-config/issues"
  }
}
