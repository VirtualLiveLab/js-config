{
  "name": "@virtual-live-lab/eslint-config",
  "version": "1.1.1",
  "description": "ESLint configuration for Virtual Live Lab",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    },
    "./js": {
      "import": "./dist/js.mjs",
      "types": "./dist/js.d.mts"
    },
    "./ts": {
      "import": "./dist/ts.mjs",
      "types": "./dist/ts.d.mts"
    },
    "./astro": {
      "import": "./dist/astro.mjs",
      "types": "./dist/astro.d.mts"
    },
    "./react": {
      "import": "./dist/react.mjs",
      "types": "./dist/react.d.mts"
    },
    "./addons/*": {
      "import": "./dist/addons/*.mjs",
      "types": "./dist/addons/*.d.mts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup-node"
  },
  "peerDependencies": {
    "eslint": "^8.57.0"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^8.57.0",
    "eslint": "^8.57.0",
    "eslint-config-flat-gitignore": "^0.1.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^0.34.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-perfectionist": "^2.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "globals": "^14.0.0",
    "pkg-dir": "^8.0.0",
    "typescript-eslint": "^7.5.0"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.43.1",
    "@types/eslint": "^8.56.7",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__eslintrc": "^2.1.1",
    "@types/eslint__js": "^8",
    "@types/node": "^20.12.7",
    "@typescript-eslint/utils": "^7.7.0",
    "@virtual-live-lab/tsconfig": "workspace:^",
    "tsup": "^8.0.2"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "provenance": true
  },
  "keywords": [],
  "author": "sushi-chaaaan <mail@sushichan.live> (https://github.com/sushi-chaaaan)",
  "homepage": "https://github.com/VirtualLiveLab/js-config/tree/main/#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/VirtualLiveLab/js-config.git",
    "directory": "packages/eslint-config"
  },
  "bugs": {
    "url": "https://github.com/VirtualLiveLab/js-config/issues"
  }
}
