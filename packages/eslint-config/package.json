{
  "name": "@virtual-live-lab/eslint-config",
  "version": "2.0.4",
  "description": "ESLint configuration for Virtual Live Lab",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    },
    "./addons/*": {
      "import": "./dist/addons/*.mjs",
      "types": "./dist/addons/*.d.mts"
    },
    "./presets/*": {
      "import": "./dist/presets/*.mjs",
      "types": "./dist/presets/*.d.mts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup-node"
  },
  "peerDependencies": {
    "eslint": "^8.57.0"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^8.57.0",
    "eslint": "^8.57.0",
    "eslint-config-flat-gitignore": "^0.1.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^1.2.2",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-perfectionist": "^2.11.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "globals": "^15.6.0",
    "pkg-dir": "^8.0.0",
    "typescript-eslint": "^7.13.1"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.10",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__eslintrc": "^2.1.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.14.8",
    "@typescript-eslint/utils": "^7.14.1",
    "@virtual-live-lab/tsconfig": "workspace:^",
    "tsup": "^8.1.0"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "provenance": true
  },
  "keywords": [],
  "author": "sushi-chaaaan <mail@sushichan.live> (https://github.com/sushi-chaaaan)",
  "homepage": "https://github.com/VirtualLiveLab/js-config/tree/main/#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/VirtualLiveLab/js-config.git",
    "directory": "packages/eslint-config"
  },
  "bugs": {
    "url": "https://github.com/VirtualLiveLab/js-config/issues"
  }
}
