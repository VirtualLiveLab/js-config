{
  "name": "@virtual-live-lab/eslint-config",
  "version": "2.2.0",
  "description": "ESLint configuration for Virtual Live Lab",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    },
    "./addons/*": {
      "import": "./dist/addons/*.mjs",
      "types": "./dist/addons/*.d.mts"
    },
    "./presets/*": {
      "import": "./dist/presets/*.mjs",
      "types": "./dist/presets/*.d.mts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup-node",
    "pkg-pr-new": "pkg-pr-new publish --compact --comment=update --pnpm"
  },
  "engines": {
    "node": ">= 20.11.0 || >= 21.2.0 || >= 22.0.0"
  },
  "peerDependencies": {
    "eslint": "^9.11.1"
  },
  "dependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.11.1",
    "@stylistic/eslint-plugin-ts": "^2.8.0",
    "@typescript-eslint/utils": "^8.7.0",
    "eslint": "^9.11.1",
    "eslint-config-flat-gitignore": "^0.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^1.2.4",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-perfectionist": "^3.7.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "globals": "^15.9.0",
    "pkg-dir": "^8.0.0",
    "typescript-eslint": "^8.7.0"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__eslintrc": "^2.1.2",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.14.15",
    "@virtual-live-lab/tsconfig": "workspace:^",
    "pkg-pr-new": "^0.0.24",
    "tsup": "^8.2.4"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "provenance": true
  },
  "keywords": [],
  "author": "sushi-chaaaan <mail@sushichan.live> (https://github.com/sushi-chaaaan)",
  "homepage": "https://github.com/VirtualLiveLab/js-config/tree/main/#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/VirtualLiveLab/js-config.git",
    "directory": "packages/eslint-config"
  },
  "bugs": {
    "url": "https://github.com/VirtualLiveLab/js-config/issues"
  }
}
