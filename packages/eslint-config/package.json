{
  "name": "@virtual-live-lab/eslint-config",
  "version": "2.3.1",
  "description": "ESLint configuration for Virtual Live Lab",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./addons/*": {
      "types": "./dist/addons/*.d.ts",
      "import": "./dist/addons/*.js"
    },
    "./presets/*": {
      "types": "./dist/presets/*.d.ts",
      "import": "./dist/presets/*.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "inspect": "eslint --inspect-config"
  },
  "engines": {
    "node": ">= 20.11.0 || >= 21.2.0 || >= 22.0.0"
  },
  "peerDependencies": {
    "eslint": "^8.57.0 || ^9.0.0"
  },
  "dependencies": {
    "@astrojs/compiler": "2.12.2",
    "@eslint/compat": "1.3.2",
    "@eslint/config-helpers": "0.3.1",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.33.0",
    "@typescript-eslint/utils": "8.40.0",
    "astro-eslint-parser": "1.2.2",
    "astrojs-compiler-sync": "1.1.1",
    "defu": "6.1.4",
    "eslint-config-flat-gitignore": "2.1.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-perfectionist": "4.15.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "6.0.0-rc.2",
    "eslint-plugin-react-refresh": "0.4.20",
    "eslint-plugin-tailwindcss": "3.18.2",
    "globals": "16.3.0",
    "package-directory": "8.1.0",
    "typescript-eslint": "8.40.0",
    "typescript-eslint-parser-for-extra-files": "0.9.0"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@virtual-live-lab/tsconfig": "workspace:*",
    "eslint": "9.37.0",
    "publint": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "unplugin-unused": "catalog:",
    "vitest": "catalog:"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "provenance": true
  },
  "keywords": [],
  "author": "VLL39 <vll39.tech@gmail.com> (https://github.com/vll39)",
  "homepage": "https://github.com/VirtualLiveLab/js-config/tree/main/#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VirtualLiveLab/js-config.git",
    "directory": "packages/eslint-config"
  },
  "bugs": {
    "url": "https://github.com/VirtualLiveLab/js-config/issues"
  }
}
